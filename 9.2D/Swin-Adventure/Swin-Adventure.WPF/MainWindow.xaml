<Window x:Class="Swin_Adventure.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Swin_Adventure.WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="720" Width="1280">
    <Grid>
        <Button x:Name="EnterButton" Content="Enter" HorizontalAlignment="Left" Margin="1130,622,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click" IsDefault="True"/>
        <ScrollViewer VerticalScrollBarVisibility="Auto" CanContentScroll="True">
            <TextBlock x:Name="GameConsole" HorizontalAlignment="Left" Margin="42,29,0,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
            </ScrollViewer>
            <TextBox x:Name="CommandBox" HorizontalAlignment="Left" Height="26" Margin="740,622,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="313"/>

    </Grid>
</Window>
